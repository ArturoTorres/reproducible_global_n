## working directory
wdir <-  "/Users/torres/Documents/10_docs_JATM/Docs-JATM-2023/16700_my_best/1612_iiasa_spatial-data-scientis_laxenburg/assignment"

## commit label
commit.label <- 'repo init - gitignore data'

## Github repository
username   <- "ArturoTorres"
password   <- github_token

repository <- "github.com/ArturoTorres/reproducible_global_n.git"

credential <- paste0("https://", username, ":", password, "@", repository)

##---------------------------------------------------------------------
## commit changes locally
##---------------------------------------------------------------------
## change to working directory
setwd(wdir)

## to see status of files
system("git status")

## to see status of ignored files
system("git status --ignored")

## to look previous status of commits
system("git log")

## create a .gitignore file to ignore folder data/
system("touch .gitignore")
system('echo "data/*" >> .gitignore')
system('echo "Assignment.pdf" >> .gitignore')

## find and write big files in .gitignore
system("find . -size +50M | cat >> .gitignore")

## to stage files
system("git add .")

## to commit changes with label
system(paste0("git commit -a -m '", commit.label, "'"))

## to see status of files
system("git status")

## to look previous status of commits
system("git log")

##---------------------------------------------------------------------
## configuring remote.origin.url for Github repository to push
##---------------------------------------------------------------------
# system(paste0("git branch -a"))

system(paste0("git config remote.origin.url ", credential))

system(paste0("git push ", credential, " master"))

#' or push to branch
system(paste0("git push ", credential, " main"))
 
system(paste0("git push ", credential, " scripts_demos_ATO"))

system(paste0("git push ", credential, " sce-ua_brin"))

#' or

system(paste0("git pull ", credential))

system(paste0("git config pull.rebase false "))

system(paste0("git pull origin main --allow-unrelated-histories "))

##---------------------------------------------------------------------
## solving "remote origin already exists"
##---------------------------------------------------------------------
# from terminal:
# git remote remove origin
# git remote add origin https://git.list.lu/agro/vineyard.git

##---------------------------------------------------------------------
## solving "push and pull buttons greyed out in RStudio"
##---------------------------------------------------------------------
# Open your console and navigate to your repo
# Make some commit
# Make a push using -u (i.e. --set-upstream) flag, eg: git push origin master -u
# Open Rstudio (or restart it if it was open while performing the previous step) and you'll see the push and pull icons are no longer greyed out.

##---------------------------------------------------------------------
## amending changing message and push (forced)
##---------------------------------------------------------------------
## to change commit message (run from terminal)
# git commit --amend

## then push (forced)
system(paste0("git push --force"))

##---------------------------------------------------------------------
## amending without changing message and push (forced)
##---------------------------------------------------------------------
## to change commit message (run from terminal)
system(paste0("git commit --amend --no-edit"))

## then push (forced)
system(paste0("git push --force"))

##---------------------------------------------------------------------
## combine two last commits in last one (rebase)
##---------------------------------------------------------------------
## If you're currently on your "commit 1", and the commit you want
## to merge, "commit 2", is the previous commit, you can run
## git rebase -i HEAD~2 and change the first word of the second line
## from "pick" to "squash". Then write your file, and quit.
## Git will squash your first commit into your second last commit.

## to rebase two last comits (run from terminal)
# git rebase -i HEAD~2

## then push (forced)
system(paste0("git push --force"))

## if fatal: The current branch master has no upstream branch.
## To push the current branch and set the remote as upstream, use:

system(paste0("git push --set-upstream origin master"))

##---------------------------------------------------------------------
## fetching and merging
##---------------------------------------------------------------------
system(paste0("git checkout master"))
system(paste0("git checkout main"))

system(paste0("git fetch ", credential))

system(paste0("git merge origin/master"))
system(paste0("git merge origin/main"))

# My work on test branch is done and I am ready to merge it back to master.
# Here are the two ways I can think of:
#
# A:
# git checkout test
# git pull origin master
# git push origin test
# git checkout master
# git pull origin test
#
# B:
# git checkout test
# git pull origin master
# git checkout master
# git merge test

##---------------------------------------------------------------------
## pulling from Github repository
##---------------------------------------------------------------------
system(paste0("git pull ", credential))

##---------------------------------------------------------------------
## untracking files (because of large size)
##---------------------------------------------------------------------
file.large <- "content/talk/2019_spa_stat/slides/slides.js"
file.large = "./script_data/model_path/dPL_SMAP/dPL_gz_s4_tr2_model.pt"

# to delete everything in the history of that file
system(paste0("git filter-branch -f --index-filter 'git rm -r --cached --ignore-unmatch ",file.large, "' HEAD"))

# to remove a file or folder from the stagging area
system(paste0("git reset HEAD -- '", file.large, "'"))


